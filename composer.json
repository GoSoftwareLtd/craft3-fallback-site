{
	"name": "charliedev/fallback-site",
	"description": "Allows sites of a multi-site install to use entries of other sites if a matching slug is found.",
	"type": "craft-plugin",
	"version": "0.0.1",
	"license": "MIT",
	"authors": [
		{
			"name": "Charlie Development",
			"email": "support@charliedev.com",
			"homepage": "http://charliedev.com/"
		}
	],
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"default",
		"element",
		"fallback",
		"site",
		"multi-site"
	],
	"support": {
		"issues": "https://github.com/charliedevelopment/craft3-fallback-site/issues?state=open",
		"source": "https://github.com/charliedevelopment/craft3-fallback-site",
		"email": "support@charliedev.com"
	},
	"require": {
		"craftcms/cms": "^3.0.0-RC1"
	},
	"autoload": {
		"psr-4": {
			"charliedev\\fallbacksite\\": "src/"
		}
	},
	"extra": {
		"name": "Fallback Site",
		"handle": "fallback-site",
		"changelogUrl": "https://raw.githubusercontent.com/charliedevelopment/craft3-fallback-site/master/CHANGELOG.md",
		"downloadUrl": "https://github.com/charliedevelopment/craft3-fallback-site/archive/master.zip",
		"schemaVersion": "0.0.1",
		"hasCpSettings": true,
		"hasCpSection": false,
		"class": "charliedev\\fallbacksite\\FallbackSite"
	}
}